<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:background="@color/white" android:id="@+id/relativeLayout1"
	android:layout_width="fill_parent" android:layout_height="fill_parent">
	<Button android:id="@+id/button1" android:text="@string/nques"
		android:layout_width="300dp" android:layout_height="wrap_content"
		android:layout_alignParentTop="true" android:layout_centerHorizontal="true"
		android:layout_marginTop="20dp"></Button>
	<Button android:text="@string/newquiz" android:layout_width="300dp"
		android:layout_height="wrap_content" android:id="@+id/button2"
		android:layout_below="@+id/button1" android:layout_alignLeft="@+id/button1"></Button>
	<Button android:text="Import Quiz from Quiz Bank"
		android:layout_width="300dp" android:layout_height="wrap_content"
		android:id="@+id/button3" android:layout_below="@+id/button2"
		android:layout_alignLeft="@+id/button2"></Button>
	<Button android:id="@+id/Button02" android:layout_width="300dp" android:layout_height="wrap_content" android:text="Exit" android:layout_below="@+id/button3" android:layout_alignLeft="@+id/button3"></Button>
		
</RelativeLayout>

